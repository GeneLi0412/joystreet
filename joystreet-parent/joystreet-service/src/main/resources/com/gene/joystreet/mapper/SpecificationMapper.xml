<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gene.joystreet.dao.SpecificationMapper">
  <resultMap id="BaseResultMap" type="com.gene.joystreet.entity.Specification">
    <id column="id" jdbcType="VARCHAR" property="id" />
    <result column="spec_name" jdbcType="VARCHAR" property="specName" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.String">
    delete from tb_specification
    where id = #{id,jdbcType=VARCHAR}
  </delete>
  <insert id="insert" parameterType="com.gene.joystreet.entity.Specification">
    insert into tb_specification (id, spec_name)
    values (#{id,jdbcType=VARCHAR}, #{specName,jdbcType=VARCHAR})
  </insert>
  <update id="updateByPrimaryKey" parameterType="com.gene.joystreet.entity.Specification">
    update tb_specification
    set spec_name = #{specName,jdbcType=VARCHAR}
    where id = #{id,jdbcType=VARCHAR}
  </update>
  <select id="selectByPrimaryKey" parameterType="java.lang.String" resultMap="BaseResultMap">
    select id, spec_name
    from tb_specification
    where id = #{id,jdbcType=VARCHAR}
  </select>
  <select id="selectAll" resultMap="BaseResultMap">
    select id, spec_name
    from tb_specification
  </select>
</mapper>